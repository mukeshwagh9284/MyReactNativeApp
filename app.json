{
  "expo": {
    "name": "MyReactNativeApp",
    "slug": "MyReactNativeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myreactnativeapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mukeshwagh.MyReactNativeApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b2d79625-7fab-4a83-8d5f-33a4499b02bd"
      }
    },
    "owner": "mukeshwagh",
    "runtimeVersion": {
      "policy": "appVersion"
    },
   "updates": {
  "url": "https://expo.dev/accounts/mukeshwagh/projects/MyReactNativeApp/builds/d25c80ba-76e7-4017-95c2-0079ce45bdcd",
  "checkAutomatically": "ON_LOAD",
  "fallbackToCacheTimeout": 0
}

  }
}
